{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp task",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "vscode-debug-start"
            ]
        },
{
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/server-start.js",
            "cwd": "${workspaceRoot}",
            "args": [ "${max_old_space_size:4000}" ],
            "sourceMaps": true,
            "preLaunchTask": "vscode-debug-start"
        
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        }
    ]
}
